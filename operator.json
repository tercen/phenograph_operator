{
  "name": "Rphenograph",
  "description": "Calclates the Jaccard coefficient between nearest-neighbor sets and identifys clusters representing phenotypic similarities between cells.",
  "tags": ["clustering", "flow cytometry", "cytof"],
  "authors": ["tercen"],
  "urls": ["https://github.com/tercen/rphenograph_operator"],
  "properties": [
  {
    "kind": "DoubleProperty",
    "name": "k",
    "defaultValue": 30,
    "description": "integer, number of nearest neighbours"
  },
  {
    "kind": "DoubleProperty",
    "name": "seed",
    "defaultValue": 42,
    "description": "integer, random seed"
  }
  ]
}


